/* pages/address/address.wxss */

.pageStyle {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f5f5f5;
}

.mapStyle {
  width: 100%;
  height: 85%;
}
.call{
  display :flex;
  justify-content: space-around;
  align-items :center;
  width :100%;
  bottom :35rpx;
  position: fixed;
}
.call .left,.call .right{
  flex :1;
  display :flex;
  flex-direction :column;
  justify-content: center;
  align-items: center;
}
.call image{
  height: 64rpx;
  width :64rpx;
}

.call  span{
  height: 50rpx;
  line-height :50rpx;
  font-size: 24rpx;
  color: #6B4F4E;
}

.footer{
  position: fixed;
  bottom :30rpx;
  height: 80rpx;
  width :716rpx;
  margin-left: 17rpx;
}
